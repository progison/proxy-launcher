{
  "name": "Proxy launcher",
  "description": "Proxy launcher for Google Chrome",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"]
}
